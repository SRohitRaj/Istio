apiVersion: config.istio.io/v1alpha2
kind: V1alpha2RouteRule
metadata:
  creationTimestamp: null
  name: FIXME
  namespace: FIXME
  resourceVersion: FIXME
spec:
  hosts:
  - c
  http:
  - corsPolicy:
      allowCredentials: true
      allowHeaders:
      - content-type
      allowMethods:
      - POST
      - GET
      - OPTIONS
      allowOrigin:
      - http://foo.example
      exposeHeaders:
      - x-custom-header
      maxAge: 300.000s
    route:
    - destination:
        name: c
        subset: version-v1
      weight: 100
