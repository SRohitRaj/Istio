apiVersion: authentication.istio.io/v1alpha1
kind: Policy
metadata:
  name: f-ports-mtls-disabled
spec:
  targets:
  - name: f
    ports:
    - number: 8080
---
apiVersion: authentication.istio.io/v1alpha1
kind: Policy
metadata:
  name: f-ports-mtls-enabled
spec:
  targets:
  - name: f
    ports:
    - number: 80
    - number: 7070
    - number: 9090
  peers:
  - mtls:
---
